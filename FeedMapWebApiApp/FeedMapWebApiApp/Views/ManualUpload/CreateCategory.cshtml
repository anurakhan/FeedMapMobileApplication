@{
    ViewBag.Title = "Create Restaurant";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<H1 class="monospaceFont">Populate Category Info</H1>

@using (Html.BeginForm("CreateCategory", "ManualUpload", FormMethod.Post, new { enctype = "multipart/form-data" })) {
    <label for="Name">Name:</label>
    @Html.TextBox("Name")<br/>

    <input type="submit" name="submit" value="Submit" />
}
